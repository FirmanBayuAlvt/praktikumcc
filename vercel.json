{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.6.2" }
    },
    "routes": [
        { "src": "/style.css", "dest": "/public/style.css" },
        { "src": "/favicon.ico", "dest": "/public/favicon.ico" },
        { "src": "/files/(.*)", "dest": "/public/files/" },
        { "src": "/images/(.*)", "dest": "/public/images/" },
        { "src": "/build/(.*)", "dest": "/public/build/" },
        { "src": "/(.*)", "dest": "/api/index.php" }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_URL": "https://cantik-public-tus.vercel.app/",
        "APP_KEY": "base64:ljhD8eq37WScrIfuZm/h1ydosYgPTx1olzzDllkEsZQ=",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",

        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie",

        "WEB_COLOR": "#3A1078",
        "WEB_NAME": "Gayungan",
        "WEB_SLUG":"kelurahan_gayungan",
        "WEB_EXT_API_URL": "https://pemerintahan.surabaya.go.id/basis-data/api/",
        "WEB_ASSET_URL": "https://pemerintahan.surabaya.go.id/basis-data/storage/",
        "WEB_EXT_API_URL_TUS": "https://cantik-internal-tus.vercel.app/api/api/",
        "WEB_ASSET_URL_TUS": "https://cantik-internal-tus.vercel.app/storage/"
    }
